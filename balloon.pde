// Draw Balloon with hardcode

void setup(){
  // set size of window 
  size(500,500);
}

void draw(){
  
  // draw balloon by using circle and line (hardcode)
  circle(250,200,50);
  line(250,225,250,275);
  
  // called noLoop() for stop the code in draw()
  noLoop();
}
